Algoritmo "Consumo"

Var

  distanciaP, combustivelG, consumoMedio: real
Inicio


      escreva("Digite a distancia pecorrida: ")
      leia(distanciaP)
      
      escreva("Combustivel pecorrido: ")
      leia(combustivelG)
      

      
      consumoMedio <- distanciaP/combustivelG
      

      
      escreva(consumoMedio: 5:3)
      

 
Fimalgoritmo